<form name="editform" action="userEdit" method="post" accept-charset="utf-8">
    <h1>Activity - Edit</h1>

    <div>
        <label>id：</label>
        <input type="text" name="id" placeholder="id" value="<%=activities[0].id %>" readonly />
    </div>
    <div>
        <label>people：</label>
        <input type="text" name="people" placeholder="people" value="<%=activities[0].people %>" />
    </div>
    <div>
        <label>group_name：</label>
        <input type="text" name="group_name" placeholder="group_name" value="<%=activities[0].group_name %>">
    </div>
    <div>
        <input type='submit' value='Submit'>
        <input type='reset' value='Reset'>
        <input type='button' value='Back' onclick="javascript: window.history.back();">
    </div>
</form>